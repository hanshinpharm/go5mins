<p id="notice"><%= notice %></p>

<h1>Listing Exos</h1>

<table>
  <thead>
    <tr>
      <th>Prdname</th>
      <th>Ownername</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @exos.each do |exo| %>
      <tr>
        <td><%= exo.PrdName %></td>
        <td><%= exo.OwnerName %></td>
        <td><%= link_to 'Show', exo %></td>
        <td><%= link_to 'Edit', edit_exo_path(exo) %></td>
        <td><%= link_to 'Destroy', exo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Exo', new_exo_path %>

