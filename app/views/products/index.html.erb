<p id="notice"><%= notice %></p>

<h1>Listing Products</h1>

<table>
  <thead>
    <tr>
      <th>Prdname</th>
      <th>Ownername</th>
      <th>Phoneno</th>
      <th>Mobileno</th>
      <th>Zipcode</th>
      <th>Address</th>
      <th>Iweight</th>
      <th>Name1</th>
      <th>Cnt1</th>
      <th>Iprice</th>
      <th>Memo</th>
      <th>Ipage</th>
      <th>Orderno</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @products.each do |product| %>
      <tr>
        <td><%= product.PrdName %></td>
        <td><%= product.OwnerName %></td>
        <td><%= product.PhoneNo %></td>
        <td><%= product.MobileNo %></td>
        <td><%= product.ZipCode %></td>
        <td><%= product.Address %></td>
        <td><%= product.iWeight %></td>
        <td><%= product.Name1 %></td>
        <td><%= product.Cnt1 %></td>
        <td><%= product.iPrice %></td>
        <td><%= product.Memo %></td>
        <td><%= product.iPage %></td>
        <td><%= product.OrderNo %></td>
        <td><%= link_to 'Show', product %></td>
        <td><%= link_to 'Edit', edit_product_path(product) %></td>
        <td><%= link_to 'Destroy', product, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Product', new_product_path %>
